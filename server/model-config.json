{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "../common/services",
      "./models",
      "./services"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "accessToken": {
    "dataSource": "mysql",
    "public": false
  },
  "acl": {
    "dataSource": "mysql",
    "public": false
  },
  "roleMapping": {
    "dataSource": "mysql",
    "public": false
  },
  "role": {
    "dataSource": "mysql",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
        }
      }
    }
  },
  "user": {
    "dataSource": "mysql",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "create": false,
          "login": false,
          "logout": true,
          "customLogin": true
        }
      }
    }
  }
}
