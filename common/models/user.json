{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {},
  "mixins": {
    "AclMixin": true,
    "RemoteMixin": true,
    "SaveMixin": {
      "properties": [
        "email","username"
      ]
    },
    "RelationsMixin": {
      "properties": [
        "roles"
      ]
    },
    "HooksMixin": true
  },
  "http": {
    "path": "user"
  },
  "scope": {
    "include": [
      {
        "relation": "roles"
      }
    ]
  },
  "properties": {
    "googleToken": {
      "type": "object"
    },
    "profile": {
      "type": "object"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "lastLoginAt": {
      "type": "date"
    },
    "ttl": {
      "type": "number",
      "default": 1800
    },
    "suspended": {
      "type": "boolean"
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    },
    "roles": {
      "type": "hasMany",
      "model": "role",
      "through": "roleMapping",
      "polymorphic": {
        "foreignKey": "principalId",
        "discriminator": "principalType"
      }
    }
  },
  "acls": [
    {
      "property": "find",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "property": "customLogin",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
